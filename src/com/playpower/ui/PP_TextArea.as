package com.playpower.ui {	import flash.display.Sprite;	import flash.events.Event;	import com.carlcalderon.arthropod.Debug;	public class PP_TextArea extends Sprite	{		public function PP_TextArea()		{			this.addEventListener(Event.ADDED_TO_STAGE, _onAddedToStage, false, 0, true);		}		public function _onAddedToStage($e:Event):void		{			this.removeEventListener(Event.ADDED_TO_STAGE, _onAddedToStage);		}	}}