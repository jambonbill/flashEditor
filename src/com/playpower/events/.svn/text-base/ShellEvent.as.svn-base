/*** @author sgrant*/package com.vday.events{	import flash.events.Event;		public class ShellEvent extends Event	{		public static const BUTTON_CLICK:String = "onButtonClick";		public static const SHOW_INFO:String = "onShowInfo";		public static const CLOSE_INFO:String = "onCloseInfo";		public static const SHOW_GALLERY:String = "onShowGallery";		public static const CLOSE_GALLERY:String = "onCloseGallery";		public static const SHOW_THUMBNAILS:String = "onShowThumbnails";		public static const CALL_SLIDESHOW:String = "onCallSlideshow";		public static const FOOTER_UPDATE:String = "onFooterUpdate";		public static const SEND_TO_FRIEND:String = "onSendToFriend";				public var objInfo:Object = {};						public function ShellEvent(type:String, objInfo:Object = null, bubbles:Boolean = false, cancelable:Boolean = false)		{	//trace("new fEvt: " + arguments);			super(type, bubbles, cancelable);						this.objInfo = objInfo;		}				public override function clone():Event		{			return new ShellEvent(type, objInfo, bubbles, cancelable);		}				public override function toString():String		{			return formatToString("ShellEvent", "type", "objInfo", "bubbles", "cancelable", "eventPhase");		}	}}