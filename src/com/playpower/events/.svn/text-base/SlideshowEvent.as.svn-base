package com.vday.events{	import flash.events.Event;	public class SlideshowEvent extends Event	{		public static const DATA_LOADED:String = "onShowDataLoaded";		public static const SHOW_LOAD_PROGRESS:String = "onShowLoadProgress";		public static const SHOW_BUFFERED:String = "onShowBuffered";		public static const SHOW_LOADED:String = "onShowLoaded";		public static const SHOW_START:String = "onShowStart";		public static const SHOW_END:String = "onShowEnd";		public static const BOOKMARK_CLICK:String = "onBookmarkClick";		public static const RESIZE:String = "onResize";				public var objInfo:Object = {};		public function SlideshowEvent(type:String,objInfo:Object = null,bubbles:Boolean=false, cancelable:Boolean=false)		{			super(type, bubbles, cancelable);			this.objInfo = objInfo		}				override public function clone():Event 		{			return new SlideshowEvent(type,objInfo,bubbles,cancelable); 		}				public override function toString():String		{			return formatToString("SlideshowEvent", "type", "objInfo", "bubbles", "cancelable", "eventPhase");		}	}}